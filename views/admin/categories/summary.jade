tr(data-id=category.id)
  td= category.name

  td.actions
    a.edit(href='/admin/categories/#{category.id}/edit', alt='edit property')
      img(src='/images/edit.png', alt='Edit')
      | Edit

  td.actions
    a.delete(href='/admin/categories/#{category.id}/delete', alt='delete property')
      img(src='/images/delete.png', alt='Delete')
      | Delete