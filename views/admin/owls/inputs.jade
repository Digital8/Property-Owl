.panes(style='display: none;', data-tab=objUser.level)
  
  .inputs.index.pane(data-key='index', data-label='details')

    h2 Property Details
    hr
      
    - if (objUser.isAdmin())

      .left
        .inputs
          label(for='title') Listed By
            | &nbsp;
            select(name="listed_by")
              each developer in developers
                - var sel = false
                  if typeof(owl.user) != 'undefined'
                    - var sel = (owl.user.user_id == developer.user_id) ? true : false;
                option(value=developer.user_id, selected=sel) #{developer.first_name} #{developer.last_name}

        .inputs
          label(for='title') Approved
            | &nbsp;
            input.big(id='approved', name='approved', type='checkbox', checked=owl.approved)
      
