tr
  td= owl.owl_id
  
  - var listed = new Date(owl.created_at)
  td #{listed.getDay()}/#{listed.getMonth()}/#{listed.getFullYear()}
  
  td= owl.address
  
  td= owl.state.toUpperCase()
  
  td
    if (owl.approved)
     | Approved
    else
     | Pending
  
  td #{owl.value} %
  
  td #{owl.registrations}
  
  //- td.actions
  //-   a(href='/admin/owls/#{owl.owl_id}/view', alt='view owl deal')
  //-     img(src='/images/view.png', alt='View')
  //-     | View
  
  td.actions
    a(href='/admin/owls/#{owl.owl_id}/edit', alt='edit owl deal')
      img(src='/images/edit.png', alt='Edit')
      | Edit
  
  td.actions
    a(href='/admin/owls/#{owl.owl_id}/delete', alt='delete owl deal')
      img(src='/images/delete.png', alt='Delete')
      | Delete