    .small-left
      img.small(src='/images/search-bed-icon.png', alt='Number of Beds')
      label(for='propertyMinBeds') Min. Beds
      select#propertyMinBeds(name="minBeds")
        option(value="any") Any
        option(value="1") 1
        option(value="2") 2
        option(value="3") 3
        option(value="4") 4
        option(value="5") 5
    
    .small-right
      img.small(src='/images/search-bed-icon.png', alt='Number of Beds')
      label(for='propertyMaxBeds') Max. Beds
      select#propertyMaxBeds(name="maxBeds")
        option(value="any") Any
        option(value="1") 1
        option(value="2") 2
        option(value="3") 3
        option(value="4") 4
