.find-deals
  h2 Find Deals
  form#input-form(action="/search",method="get")
    
    img(src='/images/search-map-icon.png', alt='Suburb')
    label(for='propertySuburb') Suburb
    input#propertySuburb(type='text',name='suburb')
    
    img(src='/images/search-map-icon.png', alt='State / Territory')
    label(for='propertyState') State / Territory
    select#propertyState(name='state')
      option(value='any') Any
      option(value='act') ACT
      option(value='nsw') NSW
      option(value='nt') NT
      option(value='qld') QLD
      option(value='sa') SA
      option(value='tas') TAS
      option(value='vic') VIC
      option(value='wa') WA
    
    img(src='/images/search-property-icon.png', alt='Property Type')
    label(for='development_type_id') Property Type
    select.development_type_id(name='development_type_id')
      option(value='') Any
      option(value='1') House
      option(value='2') Townhouse
      option(value='3') Unit
      option(value='4') Apartment
      option(value='5') Land
      option(value='6') House and Land
      option(value='7') Villa
      option(value='8') Acreage
    
